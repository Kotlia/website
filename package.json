{
  "name": "website",
  "description": "Website for New York Grace Church",
  "version": "0.0.0",
  "private": true,
  "author": "Shun Ueda <shu225@lehigh.edu>",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "precommit": "'yarn lint --fix && yarn format'",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "stylelint": "stylelint src/**/*.scss",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "fixImport": "replace '~/src/' '' src -r",
    "postinstall": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn stylelint --fix",
      "yarn lint --fix",
      "yarn format",
      "yarn fixImport"
    ]
  },
  "dependencies": {
    "@prismicio/client": "5.0.0",
    "classnames": "2.3.1",
    "hamburger-react": "2.4.1",
    "mikanjs": "^1.0.12",
    "next": "10.2.3",
    "next-images": "1.7.0",
    "prismic-reactjs": "1.3.4",
    "react": "17.0.2",
    "react-div-100vh": "0.6.0",
    "react-dom": "17.0.2",
    "react-responsive": "^8.2.0",
    "sass": "1.34.1",
    "styled-jsx": "^3.4.4"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@types/css-font-loading-module": "0.0.5",
    "@types/node": "14.17.2",
    "@types/react": "17.0.9",
    "@types/react-dom": "17.0.6",
    "@types/react-responsive": "8.0.2",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.27.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-unused-imports": "1.1.1",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.0",
    "replace": "1.2.1",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "stylelint-scss": "3.19.0",
    "typescript": "4.3.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
